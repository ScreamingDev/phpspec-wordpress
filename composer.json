{
  "name": "sourcerer-mike/phpspec-wordpress",
  "description": "WordPress PHPSpec extension",
  "keywords": [
    "BDD",
    "SpecBDD",
    "TDD",
    "spec",
    "specification",
    "tests",
    "testing"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=5.3.0",
    "phpspec/phpspec": "~2.1.1",
    "symfony/dependency-injection": "*",
    "symfony/config": "*"
  },
  "require-dev": {
    "behat/behat": "3.0",
    "bossa/phpspec2-expect": "*"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "LeMike\\PhpSpec\\WpExtension\\": "src/LeMike/PhpSpec/WpExtension"
    }
  },
  "authors": [
    {
      "name": "Mike Pretzlaw",
      "email": "pretzlaw@gmail.com"
    }
  ]
}
